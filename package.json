{
  "name": "node-basic",
  "version": "1.0.2",
  "description": "Basic Hello World",
  "main": "main.js",
  "scripts": {
    "start": "export $(./.env.sh) && nodemon --harmony node-basic.js",
    "test": "export $(./.env.sh) && TEST=true `npm bin`/mocha --require co-mocha",
    "postinstall": "(export $(./.env.sh) && if [ \"$ENV_DEV\" = \"true\" ]; then DEVELOPMENT_POSTGRES_URI=postgresql://$PGUSER:$PGPASS@$PGHOST:$PGPORT/$PGDATABASE core/db/drop-db.sh; cat core/db/*.sql |  psql; fi)"
  },
  "engines": {
    "node": "5.2.0",
    "npm": "2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://UnknownGuardian@bitbucket.org/UnknownGuardian/node-basic.git"
  },
  "dependencies": {
    "co": "^4.6.0",
    "co-pg": "^1.3.1",
    "ejs": "^2.4.1",
    "forever": "^0.15.1",
    "googleapis": "^2.1.7",
    "iconv-lite": "^0.4.13",
    "koa": "1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-ejs": "^3.0.0",
    "koa-generic-session": "^1.10.1",
    "koa-logger": "^1.3.0",
    "koa-pg": "0.24.1",
    "koa-pg-session": "^2.0.0",
    "koa-route": "^2.4.2",
    "koa-static": "^2.0.0",
    "nodemailer": "^2.0.0",
    "nodemailer-smtp-transport": "^2.6.0",
    "pg": "4.4.3",
    "pm2": "^1.1.3",
    "twilio": "^2.8.0",
    "uglify-js": "^2.6.1",
    "uuid": "^2.0.1"
  },
  "keywords": [
    "hello",
    "world"
  ],
  "author": "Matt",
  "license": "-",
  "devDependencies": {
    "cheerio": "^0.19.0",
    "co-mocha": "file:node_modules/co-mocha",
    "mocha": "^2.3.4",
    "randomstring": "^1.1.3",
    "supertest": "1.1.0"
  }
}
